{
  "compilerOptions": {
    "target": "es2015",
    "removeComments": false,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "moduleResolution": "commonjs",
    "sourceMap": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "noUnusedLocals": true
  }
}
